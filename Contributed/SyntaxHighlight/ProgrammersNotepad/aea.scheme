<?xml version="1.0" encoding="UTF-8"?>
<Scheme>
	<keyword-classes>
		<keyword-class name="aea.opcodes">
			ADD ADZ SUB SUZ MPY MPR MPZ DVP COM ABS
			CLA CLZ LDQ STO STQ ALS LLS LRS TRA TSQ
			TMI TOV AXT TIX DLY INP OUT
		</keyword-class>
