                                                                                04140310
 /* OPTIMIZES A SENTENCE OF HALMAT*/                                            04140320
OPTIMISE:                                                                       04140330
   PROCEDURE;                                                                   04140340
      DECLARE OPTTYPE(8) BIT(16) INITIAL(0,1,1,1,1,1,1,1,2);                    04141000
      DECLARE OP BIT(16);                                                       04142000
      DO WHILE OPTIMISING;                                                      04143000
         DO WHILE OPR(CTR);                                                     04144000
            CTR = CTR + 1;                                                      04145000
         END;                                                                   04146000
         CALL DECODEPOP(CTR);                                                   04147000
         IF OPTIMIZER_OFF THEN DO;                     /* OMIT OPTIMIZE*/       04148000
            IF CLASS ^= 0 THEN  CTR = SMRK_CTR;                                 04149000
            ELSE DO;                                                            04150000
               IF OPCODE = 2 THEN DO;             /*XREC*/                      04151000
                  IF TAG THEN OPTIMISING = FALSE;                               04152000
                  RETURN;                                                       04153000
               END;                                                             04154000
               ELSE IF OPCODE = 4 THEN DO;        /*SMRK*/                      04155000
                  CALL PREPARE_HALMAT(0);                                       04156000
               END;                                                             04157000
               CTR = SMRK_CTR;                                                  04158000
            END;                                                                04159000
         END;                                                                   04160000
                                                                                04161000
 /* OPTIMIZER ON*/                                                              04162000
         ELSE DO;                                                               04163000
            OP = SHR(OPR(CTR),4) & "FFF";                                       04164000
            IF OP = 4 THEN DO;    /* 4 = SMRK*/                                 04165000
               CALL PREPARE_HALMAT(0);                                          04166000
               CALL NEXTCODE;                                                   04167000
            END;                                                                04168000
                                                                                04169000
/*DR111310 - ENTER LOOP FOR END OF HALMAT RECORD (OPCODE=2)*/
/*DR111310*/IF ^CHICKEN_OUT(CTR) | OPCODE=2 THEN DO CASE OPTTYPE(CLASS);        04170000
                                                                                04171000
 /* SPECIAL CASES */                                                            04172000
               DO;                                                              04173000
                  IF OPCODE = 2 THEN DO;      /* 2 = XREC */                    04174000
                     IF TAG THEN OPTIMISING = FALSE;                            04175000
                     RETURN;                                                    04176000
                  END;                                                          04177000
                  ELSE IF OPCODE = RTRN OR OPCODE = TASN                        04178000
                     THEN GO TO EXP;                                            04179000
               END;                                                             04180000
                                                                                04181000
 /*EXPRESSION*/                                                                 04182000
               DO;                                                              04183000
EXP:              CALL GROW_TREE;                                               04184000
                  DO WHILE STILL_NODES;                                         04185000
                     CALL GET_NODE;                                             04186000
                     IF SEARCHABLE THEN DO;                                     04187000
                        IF CSE_MATCH_FOUND THEN DO;                             04188000
                           IF SETUP_REARRANGE(0) THEN DO;                       04189000
                              CALL REARRANGE_HALMAT;                            04190000
                              CALL STRIP_NODES(0);                              04191000
                              IF STATISTICS THEN DO;                            04192000
                                 CSE# = CSE# + 1;                               04193000
                                 IF OPTYPE = EXTN THEN EXTN_CSES = EXTN_CSES +1;04194000
                                 ELSE                                           04195000
                                 IF OPTYPE = TSUB THEN TSUB_CSES = TSUB_CSES +1;04196000
                                 IF TWIN_MATCH THEN MESSAGE = ' TWIN';          04197000
                                 ELSE MESSAGE = '';                             04198000
                                 OUTPUT =                                       04199000
                                    'CSE FOUND IN HAL/S STATEMENT ' || STT#     04200000
                                    || '   OPTYPE = ' || HEX(OPTYPE,4)          04201000
                                    || '  *******' || MESSAGE;                  04202000
                              END;                                              04203000
                           END;                                                 04204000
                           ELSE DO;                                             04205000
                              IF ^ARRAYED_TSUB THEN CALL EXTRACT_INVARS;        04205010
                              CALL TABLE_NODE;                                  04205020
                           END;                                                 04205030
                        END;                                                    04206000
                        ELSE DO;                                                04207000
                           IF ^ARRAYED_TSUB THEN CALL EXTRACT_INVARS;           04207010
                           CALL TABLE_NODE;                                     04207020
                        END;                                                    04207030
                     END;                                                       04208000
                  END; /*DO WHILE*/                                             04209000
                                                                                04210000
                  IF POST_STATEMENT_ZAP THEN DO;                                04211000
 /* NOT PRESENTLY NEEDED*/                                                      04211010
                     CALL ZAP_TABLES;                                           04212000
                     POST_STATEMENT_ZAP = FALSE;                                04213000
                  END;                                                          04214000
                                                                                04214010
                  ELSE IF STATEMENT_ARRAYNESS AND EXTNS_PRESENT THEN            04214020
                     CALL PURGE_EXTNS(N_INX);                                   04214030
 /* EXTN'S REMOVED AFTER ARRAYED STATEMENT*/                                    04214040
                                                                                04215000
 /* FIX BUG IN OPTIMISE LOOKING FOR ASSIGNMENT STMTS DR-107 */                  04216000
                  IF SUBCODE2 = "01" OR OPCODE = TASN                           04216010
                     THEN CALL ASSIGNMENT(CTR,0,0);                             04217000
                  CTR = SMRK_CTR;                                               04217010
               END; /*EXPRESSION*/                                              04219000
                                                                                04220000
 /*INITIALIZATION*/                                                             04221000
               CTR = SMRK_CTR;                                                  04222000
                                                                                04223000
            END; /*DO CASE*/                                                    04224000
                                                                                04224010
            IF POST_STATEMENT_PUSH THEN DO;                                     04224020
               CALL PUSH_STACK(INL#);                                           04224030
               CALL PUSH_ZAP_STACK;                                             04224040
               INL#,POST_STATEMENT_PUSH = 0;                                    04224050
               IF STACK_TRACE THEN CALL STACK_DUMP;                             04224060
            END;                                                                04224070
                                                                                04224080
            CTR = SMRK_CTR;                                                     04225000
         END;                 /* ELSE DO*/                                      04226000
      END; /* DO WHILE */                                                       04227000
   END OPTIMISE;                                                                04228000
