00650000BATCH &C JOB(P) COMMAND(&CMD) OPTION(ERROR.PROC)  OPT1(&OPT1) &OPT2
00660000BATCH &C JOB(Q) COMMAND(&CMD) OPTION(ERROR.STRUC) OPT1(&OPT1) &OPT2
00670000
00680000IF &SYSTEM. = BFS THEN DO
00690000   BATCH &C JOB(A) COMMAND(&CMD) OPTION(UNITB8V0)  OPT1(&OPT1) &OPT2
00700000   BATCH &C JOB(B) COMMAND(&CMD) OPTION(ERROR.BFS) OPT1(&OPT1) &OPT2
00710000END
