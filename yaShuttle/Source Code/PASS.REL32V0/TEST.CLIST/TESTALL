00010000/********************************************************************/
00020000/*------------------------------------------------------------------*/
00030000/*                                                                  */
00040000/*   NAME:     TESTALL                                              */
00050000/*                                                                  */
00060000/*------------------------------------------------------------------*/
00070000/*                                                                  */
00080000/*   FUNCTION: DRIVER CLIST TO BATCH CLIST T0 SUBMIT BATCH JOBS TO  */
00090000/*             EXECUTE THE COMMAND TO RUN ALL FUNCTIONAL TESTS.     */
00091000/*             COMPOOL MUST BE RUN BEFORE TESTALL IS SUBMITTED.     */
00091100/*                                                                  */
00092000/*------------------------------------------------------------------*/
00093000/*                                                                  */
00094000/*   REVISION HISTORY:                                              */
00095000/*                                                                  */
00096000/*  DATE      PCR  NAME  DESCRIPTION                                */
00097000/*  --------  ---  ---   -----------------------------------------  */
00098008/*  09/10/02  730  LWW   BASELINED COMPILER TEST CLIST              */
00099000/*------------------------------------------------------------------*/
00100000/********************************************************************/
00110000
00120008PROC 0 CMD(T32V0)          +
00130000       OPT1(FIRSTRUN)      +
00130100       OPT2()              +
00131000       C(T)                +
00140000       SYSTEM(PASS)
00150000
00160000GLOBAL RETCODE
00160100IF &OPT2. NE THEN SET &OPT2 = OPT2(&OPT2.)
00160200             ELSE SET &OPT2 =
00180000BATCH &C JOB(A) COMMAND(&CMD) OPTION(ARRAY)   OPT1(&OPT1) &OPT2
00190000BATCH &C JOB(B) COMMAND(&CMD) OPTION(BIT)     OPT1(&OPT1) &OPT2
00200000BATCH &C JOB(C) COMMAND(&CMD) OPTION(BLOCK)   OPT1(&OPT1) &OPT2
00210000BATCH &C JOB(D) COMMAND(&CMD) OPTION(BLTIN)   OPT1(&OPT1) &OPT2
00220000BATCH &C JOB(E) COMMAND(&CMD) OPTION(CHAR)    OPT1(&OPT1) &OPT2
00230000BATCH &C JOB(F) COMMAND(&CMD) OPTION(DECL)    OPT1(&OPT1) &OPT2
00240000BATCH &C JOB(G) COMMAND(&CMD) OPTION(FLOW)    OPT1(&OPT1) &OPT2
00250000BATCH &C JOB(H) COMMAND(&CMD) OPTION(IO)      OPT1(&OPT1) &OPT2
00260000BATCH &C JOB(I) COMMAND(&CMD) OPTION(IS)      OPT1(&OPT1) &OPT2
00270000BATCH &C JOB(J) COMMAND(&CMD) OPTION(MACRO)   OPT1(&OPT1) &OPT2
00280000BATCH &C JOB(K) COMMAND(&CMD) OPTION(MISC)    OPT1(&OPT1) &OPT2
00290000BATCH &C JOB(L) COMMAND(&CMD) OPTION(MV)      OPT1(&OPT1) &OPT2
00300000BATCH &C JOB(M) COMMAND(&CMD) OPTION(NAME)    OPT1(&OPT1) &OPT2
00310000BATCH &C JOB(N) COMMAND(&CMD) OPTION(PROC)    OPT1(&OPT1) &OPT2
00320000BATCH &C JOB(O) COMMAND(&CMD) OPTION(STRUC)   OPT1(&OPT1) &OPT2
00330000BATCH &C JOB(P) COMMAND(&CMD) OPTION(UNIT23V0) OPT1(&OPT1) &OPT2
00340000BATCH &C JOB(Q) COMMAND(&CMD) OPTION(UNIT23V1) OPT1(&OPT1) &OPT2
00350000BATCH &C JOB(A) COMMAND(&CMD) OPTION(UNIT23V2) OPT1(&OPT1) &OPT2
00360000BATCH &C JOB(B) COMMAND(&CMD) OPTION(UNIT24V0) OPT1(&OPT1) &OPT2
00370000BATCH &C JOB(C) COMMAND(&CMD) OPTION(UNIT24V1) OPT1(&OPT1) &OPT2
